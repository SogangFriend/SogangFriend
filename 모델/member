#models.py
from django.db import models


class member(models.Model):
    name = models.CharField(max_length=30,blank=False)#blank랑 null차이?
    student_number = models.IntegerField(blank=True)#choices(select박스)
    email = models.EmailField(blank=False)
    password = models.CharField(max_length=10,blank=False)
    introduction = models.TextField(blank=True)#help_text(필드입력도움말기능)
